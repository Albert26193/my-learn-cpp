{
    "tasks": [
        {
            "type": "shell",
            "label": "cmake-configure",
            "command": "tools/cmake-3.28.0-rc5-linux-x86_64/bin/cmake",
            "args": [
                "-S",
                ".",
                "-B",
                "build"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            }
        },
        {
            "type": "shell",
            "label": "cmake-build",
            "command": "tools/cmake-3.28.0-rc5-linux-x86_64/bin/cmake",
            "args": [
                "--build",
                "build"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "dependsOn": [
                "cmake-configure"
            ]
        }
    ],
    "version": "2.0.0"
}
